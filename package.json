{
  "name": "api-testing-project",
  "version": "1.3.0",
  "description": "Comprehensive API Testing Suite for ERP System",
  "scripts": {
    "test:CRUD": "npm test -- tests/comprehensive-lifecycle/1.comprehensive-CRUD-Validation.test.js --verbose",
    "test:Security": "npm test -- tests/comprehensive-lifecycle/2.comprehensive-API-Security.test.js --verbose",
    "test:Performance": "npm test -- tests/comprehensive-lifecycle/4.Performance-Malicious-Load.test.js --verbose",
    "test:Health": "npm test -- tests/comprehensive-lifecycle/5.API-Health-Checks.test.js --verbose",
    "test:all-modules": "node run-all-tests-with-report.js",
    "test:all-sequential": "npx jest tests/comprehensive-lifecycle/1.comprehensive-CRUD-Validation.test.js --config=jest.config.js && npx jest tests/comprehensive-lifecycle/2.comprehensive-API-Security.test.js --config=jest.config.js && npx jest tests/comprehensive-lifecycle/3.Advanced-Security-Testing.test.js --config=jest.config.js && npx jest tests/comprehensive-lifecycle/4.Performance-Malicious-Load.test.js --config=jest.config.js && npx jest tests/comprehensive-lifecycle/5.API-Health-Checks.test.js --config=jest.config.js",
    "test:failed": "npx jest --onlyFailures",
    "test:failures-only": "npx jest --silent --testFailureExitCode=0 --json --outputFile=test-results.json && node utils/show-failures.js",
    "test:rerun-failed": "npx jest --passWithNoTests --lastFailed",
    "report:failed": "node utils/filter-failed-tests.js",
    "test:quick-fail": "npx jest --bail --findRelatedTests",
    "test:fail-fast": "npx jest --bail=1 --verbose",
    "test:orchestrated": "node utils/test-orchestrator.js",
    "crud-html": "npx jest --runInBand tests/comprehensive-lifecycle/1.comprehensive-CRUD-Validation.test.js --config=jest.config.js",
    "crud-minimal": "npx jest --runInBand --no-cache tests/comprehensive-lifecycle/1.comprehensive-CRUD-Validation.test.js --config=jest.config.minimal.js",
    "test-debug": "npx jest --runInBand --no-cache --verbose --detectOpenHandles tests/comprehensive-lifecycle/1.comprehensive-CRUD-Validation.test.js --config=jest.config.js",
    "test:no-babel": "npx jest --runInBand --no-cache --config=jest.config.no-babel.js tests/comprehensive-lifecycle/1.comprehensive-CRUD-Validation.test.js",
    "test:simple": "npx jest test-simple.js --no-cache --config=jest.config.no-babel.js",
    "install-deps": "npm install --save-dev @babel/core @babel/preset-env babel-jest",
    "crud": "npx jest tests/comprehensive-lifecycle/crud.test.js",
    "create-crude": "npx jest tests/comprehensive-lifecycle/crud.test.js -t 'CREATE CRUD' --verbose",
    "debug-token-status": "node debug-token-status.js",
    "debug-token-issue": "node debug-token-issue.js",
    "fix-token": "node fix-token-file.js",
    "fetch-token": "node fetchToken.js",
    "check-token": "node check-token.js",
    "debug-token": "node debug-token.js",
    "clean:reports": "npx rimraf jest-html-reporters-attach html-report coverage",
    "test": "jest --runInBand --config=jest.config.js",
    "test:html": "jest --runInBand --config=jest.config.js",
    "test:report": "npm run test:html",
    "test:ci": "npm run test:html",
    "verify:setup": "node -e \"console.log('âœ… Setup verified - Run npm run test:report');\"",
    "schema:update": "node scripts/update-all-schemas.js",
    "schema:convert-urls": "node scripts/update-schemas-to-extensions.js",
    "schema:fix-non-urls": "node scripts/fix-schema-non-urls.js",
    "registry:stats": "node scripts/query-id-registry.js stats",
    "registry:list": "node scripts/query-id-registry.js list",
    "registry:report": "node scripts/query-id-registry.js report",
    "registry:export": "node scripts/query-id-registry.js export",
    "registry:active": "node scripts/query-id-registry.js active",
    "registry:recent": "node scripts/query-id-registry.js recent"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "axios": "^1.13.1",
    "babel-jest": "^30.2.0",
    "dotenv": "^17.2.3",
    "jest": "^28.1.3",
    "jest-html-reporter": "^4.3.0",
    "jest-html-reporters": "^3.1.7",
    "rimraf": "^6.1.0"
  },
  "dependencies": {
    "playwright": "^1.56.1"
  }
}